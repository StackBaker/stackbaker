{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "StackBaker",
    "version": "0.0.2"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.stackbaker.dev",
      "targets": "all",
      "category": "Productivity"
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": true,
      "endpoints": [
        "https://www.stackbaker.com/_files"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEUwMDAxMDhGN0I3NEQ4QkEKUldTNjJIUjdqeEFBNEJGV0FuZkVFMGRnRVZCZTZDcmpYQ3pKaW9aSjh5OHhoVFJBajdxMzU4YlIK"
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "StackBaker",
        "width": 1366,
        "height": 768
      }
    ]
  }
}
